{
    "manifest_version": 2,
    "name": "Tweet Plagarism Checker",
    "description": "Tool to detect plagirism in Twitter Tweets",
    "version": "0.1",
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon":  "icon.png",
        "default_popup": "popup.html"
        
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}