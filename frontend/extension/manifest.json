{
    "manifest_version": 2,
    "name": "Tweet Plagarism Checker",
    "description": "",
    "version": "0.1",
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon":  "icon.png",
        "default_popup": "popup.html"
        
    },
    "permissions": [
        "tabs",
        "https://twitter.com/*",
        "http://twitter.com/*"
    ]
}